int RELE1 = 2;      // Definir pin digital del primer rele
int RELE2 = 3;      // Definir pin digital del segundo rele
int RELE3 = 4;      // Definir pin digital del tercer rele
int RELE4 = 5;      // Definir pin digital del cuarto rele
int RELE5 = 6;      // Definir pin digital del quinto rele
int RELE6 = 7;      // Definir pin digital del sexto rele
int RELE7 = 8;      // Definir pin digital del septimo rele
int RELE8 = 9;      // Definir pin digital del octavo rele
int RELE9 = 10;      // Definir pin digital del primer rele
int RELE10 = 11;      // Definir pin digital del segundo rele
int RELE11 = 12;      // Definir pin digital del tercer rele
int RELE12 = 13;      // Definir pin digital del cuarto rele
int RELE13 = 14;      // Definir pin digital del quinto rele
int RELE14 = 15;      // Definir pin digital del sexto rele
int RELE15 = 16;      // Definir pin digital del septimo rele
int RELE16 = 17;      // Definir pin digital del octavo rele

void setup(){
  pinMode(RELE1, OUTPUT);  // Definimos como salida
  pinMode(RELE2, OUTPUT);  // Definimos como salida
  pinMode(RELE3, OUTPUT);  // Definimos como salida
  pinMode(RELE4, OUTPUT);  // Definimos como salida
  pinMode(RELE5, OUTPUT);  // Definimos como salida
  pinMode(RELE6, OUTPUT);  // Definimos como salida
  pinMode(RELE7, OUTPUT);  // Definimos como salida
  pinMode(RELE8, OUTPUT);  // Definimos como salida
  pinMode(RELE9, OUTPUT);  // Definimos como salida
  pinMode(RELE10, OUTPUT);  // Definimos como salida
  pinMode(RELE11, OUTPUT);  // Definimos como salida
  pinMode(RELE12, OUTPUT);  // Definimos como salida
  pinMode(RELE13, OUTPUT);  // Definimos como salida
  pinMode(RELE14, OUTPUT);  // Definimos como salida
  pinMode(RELE15, OUTPUT);  // Definimos como salida
  pinMode(RELE16, OUTPUT);  // Definimos como salida
}

void loop(){
  
  digitalWrite(RELE1, LOW);  // Activacion del modulo de rele con un nivel bajo
  delay(500);      // Demora de 5 seg.
  digitalWrite(RELE1, HIGH); // Apagado del modulo de rele con un nivel alto
  delay(500);      // Demora de 5 seg.

  digitalWrite(RELE2, LOW);  // Activacion del modulo de rele con un nivel bajo
  delay(500);      // Demora de 5 seg.
  digitalWrite(RELE2, HIGH); // Apagado del modulo de rele con un nivel alto
  delay(500);      // Demora de 5 seg.

