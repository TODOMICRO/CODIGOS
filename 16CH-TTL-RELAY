  delay(500);      // Demora de 5 seg.
  digitalWrite(RELE12, HIGH); // Apagado del modulo de rele con un nivel alto
  delay(500);      // Demora de 5 seg.

  digitalWrite(RELE13, LOW);  // Activacion del modulo de rele con un nivel bajo
  delay(500);      // Demora de 5 seg.
  digitalWrite(RELE13, HIGH); // Apagado del modulo de rele con un nivel alto
  delay(500);      // Demora de 5 seg.

  digitalWrite(RELE14, LOW);  // Activacion del modulo de rele con un nivel bajo
  delay(500);      // Demora de 5 seg.
