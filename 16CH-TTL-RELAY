  delay(500);      // Demora de 5 seg.
  digitalWrite(RELE7, HIGH); // Apagado del modulo de rele con un nivel alto
  delay(500);      // Demora de 5 seg.

  digitalWrite(RELE8, LOW);  // Activacion del modulo de rele con un nivel bajo
  delay(500);      // Demora de 5 seg.
  digitalWrite(RELE8, HIGH); // Apagado del modulo de rele con un nivel alto
  delay(500);      // Demora de 5 seg.

  digitalWrite(RELE9, LOW);  // Activacion del modulo de rele con un nivel bajo
  delay(500);      // Demora de 5 seg.
  digitalWrite(RELE9, HIGH); // Apagado del modulo de rele con un nivel alto
  delay(500);      // Demora de 5 seg.

  digitalWrite(RELE10, LOW);  // Activacion del modulo de rele con un nivel bajo
  delay(500);      // Demora de 5 seg.
