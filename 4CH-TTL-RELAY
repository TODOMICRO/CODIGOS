int RELE1 = 2;      // Definir pin digital del primer rele
int RELE2 = 3;      // Definir pin digital del segundo rele
int RELE3 = 4;      // Definir pin digital del tercer rele
int RELE4 = 5;      // Definir pin digital del cuarto rele

void setup(){
  pinMode(RELE1, OUTPUT);  // Definimos como salida
  pinMode(RELE2, OUTPUT);  // Definimos como salida
  pinMode(RELE3, OUTPUT);  // Definimos como salida
  pinMode(RELE4, OUTPUT);  // Definimos como salida
}

void loop(){
  
  digitalWrite(RELE1, LOW);  // Activacion del modulo de rele con un nivel bajo
  delay(500);      // Demora de 5 seg.
  digitalWrite(RELE1, HIGH); // Apagado del modulo de rele con un nivel alto
  delay(500);      // Demora de 5 seg.

  digitalWrite(RELE2, LOW);  // Activacion del modulo de rele con un nivel bajo
  delay(500);      // Demora de 5 seg.
  digitalWrite(RELE2, HIGH); // Apagado del modulo de rele con un nivel alto
  delay(500);      // Demora de 5 seg.

  digitalWrite(RELE3, LOW);  // Activacion del modulo de rele con un nivel bajo
  delay(500);      // Demora de 5 seg.
  digitalWrite(RELE3, HIGH); // Apagado del modulo de rele con un nivel alto
  delay(500);      // Demora de 5 seg.

  digitalWrite(RELE4, LOW);  // Activacion del modulo de rele con un nivel bajo
  delay(500);      // Demora de 5 seg.
  digitalWrite(RELE4, HIGH); // Apagado del modulo de rele con un nivel alto
  delay(500);      // Demora de 5 seg.

}
